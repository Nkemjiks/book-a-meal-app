{
  "name": "book-a-meal-app",
  "version": "1.0.0",
  "description": "Book-A-Meal is an application that allows customers to make food orders and helps the foodÂ vendor know what the customers want to eat.",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/server.js --exec babel-node",
    "e2e-server": "cross-env NODE_ENV=test nodemon server/server.js --exec babel-node",
    "client-dev": "webpack-dev-server --mode development",
    "dev-server": "concurrently 'npm run client-dev' 'npm run dev'",
    "e2e-dev-server": "concurrently 'npm run client-dev' 'npm run e2e-server'",
    "build-server": "babel server -d dist",
    "build-client": "webpack --mode production",
    "build": "npm run build-client && npm run build-server",
    "start": "NODE_ENV=production npm run build && node dist/server.js",
    "test-server": "NODE_ENV=test npm run migrate-seed && cross-env NODE_ENV=test nyc mocha server/test/*.js --exit --timeout 15000",
    "migrate-seed": "sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed --seed 20180710040013-users.js 20180712142236-meal.js",
    "migrate-seed-e2e": "sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all",
    "test-client": " NODE_ENV=test jest --config=jest.config.js",
    "test": "concurrently 'npm run test-server' 'npm run test-client'",
    "test:watch": "npm run test-client -- --watch",
    "test-e2e": "NODE_ENV=test npm run migrate-seed-e2e && nightwatch --config nightwatch.json --verbose",
    "e2e-setup": "selenium-standalone install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nkemjiks/book-a-meal-app.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Nkemjiks/book-a-meal-app/issues"
  },
  "homepage": "https://github.com/Nkemjiks/book-a-meal-app#readme",
  "nyc": {
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "require": [
      "babel-register"
    ],
    "exclude": [
      "server/test/*.js",
      "server/test/mock/*.js",
      "server/database/*.js",
      "server/helpers/*.js"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {
    "axios": "^0.21.1",
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "dotenv-webpack": "^1.5.7",
    "enzyme": "^3.3.0",
