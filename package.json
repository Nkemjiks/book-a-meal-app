  },
  "homepage": "https://github.com/Nkemjiks/book-a-meal-app#readme",
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "exclude": [
      "server/test/*.js",
      "server/test/mock/*.js",
      "server/database/*.js",
      "server/helpers/*.js"
    ],
    "sourceMap": false,
    "instrument": false
  },
